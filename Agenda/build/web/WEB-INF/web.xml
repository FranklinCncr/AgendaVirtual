<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>InicioSesion</servlet-name>
        <servlet-class>Servlet.InicioSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarUsuarios</servlet-name>
        <servlet-class>Servlet.RegistrarUsuarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NuevoContacto</servlet-name>
        <servlet-class>Servlet.NuevoContacto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NuevoProyecto</servlet-name>
        <servlet-class>Servlet.NuevoProyecto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearProyecto</servlet-name>
        <servlet-class>Servlet.CrearProyecto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ContactoN</servlet-name>
        <servlet-class>Servlet.ContactoN</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditContactos</servlet-name>
        <servlet-class>Servlet.EditContactos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarProyecto</servlet-name>
        <servlet-class>Servlet.EditarProyecto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>eliminarproyecto</servlet-name>
        <servlet-class>Servlet.eliminarproyecto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarContacto</servlet-name>
        <servlet-class>Servlet.EliminarContacto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearProyectoC</servlet-name>
        <servlet-class>Servlet.CrearProyectoC</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InicioSesion</servlet-name>
        <url-pattern>/iniciar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarUsuarios</servlet-name>
        <url-pattern>/nuevousuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NuevoProyecto</servlet-name>
        <url-pattern>/NuevoProyecto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NuevoContacto</servlet-name>
        <url-pattern>/NuevoContacto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearProyecto</servlet-name>
        <url-pattern>/crearp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContactoN</servlet-name>
        <url-pattern>/contacton</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditContactos</servlet-name>
        <url-pattern>/editcontactos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarProyecto</servlet-name>
        <url-pattern>/editarp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>eliminarproyecto</servlet-name>
        <url-pattern>/eproyecto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarContacto</servlet-name>
        <url-pattern>/econtacto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearProyectoC</servlet-name>
        <url-pattern>/crearpc</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
